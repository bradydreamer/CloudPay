<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
    
	<TableRow 
        android:layout_marginBottom="0dp"
        >
        <ImageView 
            android:src="@drawable/small_table_number_input_7"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber7"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_8"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber8"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_9"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber9"
            />
        
        <ImageView 
	        android:src="@drawable/small_table_number_input_back"
	        style="@style/transfer_input_btn_style_common"
	        android:onClick="onClickBtnNumberBack"
         	/>
    </TableRow>
    
    <TableRow 
        android:layout_marginBottom="0dp"
        >
        <ImageView 
            android:src="@drawable/small_table_number_input_4"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber4"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_5"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber5"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_6"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber6"
            />
        
        
        <ImageView 
	         android:src="@drawable/small_table_number_input_c"
	         style="@style/transfer_input_btn_style_common"
	         android:onClick="onClickBtnC"
	         />
    </TableRow>
    
    <TableRow android:layout_marginBottom="0dp"
        >

        <ImageView 
            android:src="@drawable/small_table_number_input_1"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber1"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_2"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber2"
            />
        <ImageView 
            android:src="@drawable/small_table_number_input_3"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber3"
            />
        <ImageView 
            android:id="@+id/btnXImageView"
            android:src="@drawable/small_table_number_input_x"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnX"
            />
                
        
    </TableRow>
    <TableRow android:layout_marginBottom="0dp">
        <ImageView 
	        android:src="@drawable/small_table_number_input_0"
	        style="@style/transfer_input_btn_style_common"
	        android:onClick="onClickBtnNumber0"
         	/>
        <ImageView 
            android:src="@drawable/small_table_number_input_00"
            style="@style/transfer_input_btn_style_common"
            android:onClick="onClickBtnNumber00"
            />
        <ImageView 
	        android:src="@drawable/small_table_number_input_ok"
	        android:layout_span="2"
	        style="@style/transfer_input_btn_style_common"
	        android:onClick="onClickBtnOK"
	        />
    </TableRow>
</TableLayout>
