<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="20dp" >

        <LinearLayout style="@style/module_layout"
            android:visibility="gone">

            <ImageButton
                style="@style/module_logo"
                android:background="@drawable/logo_set_merch_id"
                android:onClick="gotoSetMerchId" />

            <TextView
                style="@style/module_title"
                android:text="@string/settins_index_module_title_setMerchId" />
        </LinearLayout>

        <LinearLayout style="@style/module_layout" >

            <ImageButton
                style="@style/module_logo"
                android:background="@drawable/logo_pre_authorization"
                android:onClick="gotoListUsers" />

            <TextView
                style="@style/module_title"
                android:text="@string/settins_index_module_title_list_users" />
        </LinearLayout>
         <LinearLayout style="@style/module_layout" >

            
        </LinearLayout>
         <LinearLayout style="@style/module_layout" >

            
        </LinearLayout>
       

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:weightSum="4" >
        
		<LinearLayout style="@style/module_layout">
			 <FrameLayout
                style="@style/module_logo">
             </FrameLayout>

            <TextView
                style="@style/module_title"/>
            
        </LinearLayout>	    
         
    </TableRow>

</TableLayout>
